  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/nflow-dev/cypress.config.ts",
        "baseUrl": "https://dev.ncore.dev.v2.nuclent.com",
        "testingType": "e2e"
